# GitHub Runner Environment Variables
# Copy this file to .env and fill in your values

# Required: GitHub Personal Access Token with 'repo' and 'workflow' permissions
GITHUB_TOKEN=your_github_token_here

# Required: GitHub repository in format owner/repo
GITHUB_REPOSITORY=slagathor34/awx-azure-windows-ee

# Optional: Custom runner name (defaults to docker-runner-<hostname>)
RUNNER_NAME=docker-runner-rhel-windows

# Optional: Custom runner labels (comma-separated)
RUNNER_LABELS=self-hosted,docker,linux,x64,rhel-windows-ee
